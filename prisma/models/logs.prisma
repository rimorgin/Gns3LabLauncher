model Logs {
  id         Int       @id @default(autoincrement())
  level      String?   @db.VarChar
  timestamp  DateTime? @db.Timestamp(6)
  message    String?
  context    String?   @db.VarChar
  stack      Json?     @db.Json
  statusCode Int
  durationMs Int
  ip         String?   @db.VarChar

  @@index([timestamp])
  @@index([level])
  @@index([context])
}
